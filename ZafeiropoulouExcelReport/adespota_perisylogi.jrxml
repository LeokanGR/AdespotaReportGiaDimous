	<queryString>
		<![CDATA[select p.PERISYLLOGHDATE, p.PERISYLLOGHAPO
from strayanimals.kinhsh as k, strayanimals.perisyllogh as p, strayanimals.t013_municipality as m
where m.ID = p.DIMOS_ID
and k.dimos_id = $P{p_dimos_id}
and p.PERISYLLOGHAPO LIKE 'Δήμο'
and p.KINHSH_ID = k.id
and p.perisylloghdate between $P{fromdate} and $P{todate}
and k.drasi_id =1
and k.microchip = $P{chip}
union all
select p2.PERISYLLOGHDATE, p2.PERISYLLOGHAPO
from strayanimals.kinhsh AS k2, strayanimals.perisyllogh as p2
where p2.PERISYLLOGHAPO LIKE 'Ιδιωτικό συνεργείο/ Ιδιώτη'
and k2.dimos_id = $P{p_dimos_id}
and p2.KINHSH_ID = k2.id
and p2.perisylloghdate between $P{fromdate} and $P{todate}
and k2.drasi_id =1
and k2.microchip = $P{chip}
union all
select p3.PERISYLLOGHDATE, p3.PERISYLLOGHAPO
from strayanimals.kinhsh as k3, strayanimals.perisyllogh as p3, strayanimals.filozwiko_swmateio AS fs3
where p3.SWMATEIO_ID = fs3.ID
and p3.PERISYLLOGHAPO LIKE 'Φιλοζωικό Σωματείο'
and k3.dimos_id = $P{p_dimos_id}
and  p3.KINHSH_ID = k3.id
and p3.perisylloghdate between $P{fromdate} and $P{todate}
and k3.drasi_id =1
and k3.microchip = $P{chip}]]>
	</queryString>
